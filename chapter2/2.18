(define (length items)
    (define (length-iter a count)
        (if (null? a)
            count
            (length-iter (cdr a) (+ 1 count))))
    (length-iter items 0)

(define (reverse items)
  (define (reverse-iter n)
    (if (< n 0)
      nil
      (cons (list-ref items n) (reverse-iter (- n 1))))
  (reverse-iter (length items))
